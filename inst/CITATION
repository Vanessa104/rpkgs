citHeader("To cite rpkgs in publications use:")

citEntry(
  entry    = "Article",
  title    = "A Skeleton R Package",
  author   = "Anthony Federico",
  journal  = "Monti Lab",
  year     = "2020",
  volume   = "1",
  number   = "1",
  pages    = "1-2",
  url      = "https://github.com/montilab/rpkgs",
  textVersion = paste(
    "A Skeleton R Package (2020) Monti Lab."
  )
)
